{
	"name": "@popup-maker/core-data",
	"version": "1.0.0-beta.5",
	"description": "Core data stores for Popup Maker.",
	"author": "Code Atlantic",
	"license": "GPL-2.0-or-later",
	"keywords": [
		"wordpress",
		"popup-maker",
		"data",
		"redux"
	],
	"homepage": "https://github.com/popupmaker/popup-maker/tree/trunk/packages/core-data#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/popupmaker/popup-maker.git",
		"directory": "packages/core-data"
	},
	"bugs": {
		"url": "https://github.com/popupmaker/popup-maker/issues"
	},
	"main": "build/index.js",
	"types": "build-types/index.d.ts",
	"exports": {
		".": {
			"types": "./build-types/index.d.ts",
			"default": "./build/index.js"
		}
	},
	"wpScript": true,
	"sideEffects": false,
	"dependencies": {
		"@popup-maker/i18n": "*",
		"@wordpress/api-fetch": "^7.26.0",
		"@wordpress/core-data": "^7.26.0",
		"@wordpress/data": "^10.26.0",
		"@wordpress/element": "^6.26.0",
		"@wordpress/hooks": "^4.26.0",
		"@wordpress/html-entities": "^4.26.0",
		"@wordpress/notices": "^5.26.0",
		"fast-json-patch": "^3.1.1",
		"lodash": "^4.17.21"
	},
	"scripts": {
		"build:tsc": "tsc",
		"build:types": "tsc --build",
		"watch:tsc": "tsc -w",
		"clean": "rimraf build build-types tsconfig.tsbuildinfo",
		"lint": "wp-scripts lint-js .",
		"format": "wp-scripts lint-js . --fix",
		"packages-update": "wp-scripts packages-update"
	},
	"publishConfig": {
		"access": "public"
	},
	"files": [
		"build",
		"build-types",
		"src",
		"tsconfig.json",
		"README.md",
		"LICENSE"
	]
}
